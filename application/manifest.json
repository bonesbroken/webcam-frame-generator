{
    "version": "1.1.7",
    "name": "Webcam Frame Generator",
    "buildPath": "dist/",
    "permissions": ["slobs.scenes-sources"],
    "supportedPlatforms": ["twitch", "youtube", "facebook"],
    "icon": "./images/app-sq.jpg",
    "sources": [
      {
        "type": "browser_source",
        "name": "Webcam Frame",
        "id": "webcam-frame-source",
        "about": {
          "description": "Generate endless beautiful masks and frames for your Webcam!",
          "bannerImage": "./images/app-icon.jpg"
        },
        "file": "index.html",
        "initialSize": {
          "type": "absolute",
          "width": 1280,
          "height": 720
        }
      }
    ],
    "pages": [
      {
        "slot": "top_nav",
        "file": "settings.html",
        "persistent": true,
        "allowPopout": true,
        "popOutSize": {
          "width": 600,
          "height": 600
        }
      }
    ]
  }